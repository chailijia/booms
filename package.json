{
  "name": "booms",
  "version": "1.2.9",
  "description": "A high performance and easy-to-use RPC microservices framework for Node.js. With Booms, we can load a directory as a service, call the remote functions in it like s1.say.hi(), same as we do it with local files. Booms is based on gRPC-node, but it does not require you to write proto files, which is more easier to use.\n",
  "main": "index.js",
  "scripts": {
    "test": "cd test && mocha . --exit"
  },
  "keywords": [
    "rpc",
    "microservices",
    "gRPC",
    "gRPC-node",
    "remote procedure call",
    "node framework",
    "rpc framework",
    "microservices framework"
  ],
  "author": "hi.owen.luke@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@grpc/proto-loader": "^0.1.0",
    "caller": "^1.0.1",
    "fs-extra": "^8.1.0",
    "google-protobuf": "^3.0.0",
    "grpc": "^1.11.0",
    "ioredis": "^4.14.1",
    "kdo": "^1.8.1",
    "keypaths": "^0.1.3",
    "lodash": "^4.6.1",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "lodash": "^4.17.15",
    "mocha": "^6.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiowenluke/booms.git"
  },
